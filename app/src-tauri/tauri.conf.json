{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "after",
  "version": "0.1.0",
  "identifier": "com.hoube.ark",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "A.F.T.E.R",
        "width": 1280,
        "height": 800,
        "resizable": true,
        "fullscreen": false,
        "dragDropEnabled": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/logo.ico",
      "icons/logo.ico",
      "icons/logo.ico",
      "icons/logo.ico",
      "icons/logo.ico"
    ]
  }
}
