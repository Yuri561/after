{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "after",
  "version": "0.1.0",
  "identifier": "com.hoube.ark",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null,
      "capabilities": ["default"]
    },
    "windows": [
      {
        "label": "main",
        "title": "A.F.T.E.R",
        "width": 1400,
        "height": 1024,
        "dragDropEnabled": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}
